<!--pages/scanBind/scanBind.wxml-->
<view
  class="bind-box"
  style="background-image:url({{bgImg}})">
  <view class="content">
    <view class="head-title fc-ccc fs12">您即将绑定的电子车牌卡号为</view>
    <view class="head-title fs16">{{plateNo}}</view>
  </view>
</view>
<picker bindchange="bindPickerChange" range="{{brandList}}">
  <fixBtn bindtap="confirmBtn">确认绑定</fixBtn>
</picker>
