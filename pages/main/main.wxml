<!--pages/main/main.wxml-->
<view class="linear-box">
  <top-bg></top-bg>
  <view class="card-show-box mb-30" style="padding-top:{{navBarHeight+10}}px;">
    <navigation-area current-place="{{currentPlace}}"></navigation-area>
    <card-show bindgetCarNum="getCarNum"></card-show>
  </view>
  <!-- 导航 -->
  <busniss-gird list="{{gridIcon}}" class="mg-30"></busniss-gird>
  <!-- 电子车牌 车辆信息 扫一扫按钮 -->
  <group-btn class="mg-30" bindelecClick="elecBrandBtn" bindcarInfoClick="carInfoBtn" bindscanClick="scanBtn"></group-btn>
  <!-- 活动轮播 -->
  <banner class="mb-30" bannerId="{{bannerId}}"></banner>
</view>

