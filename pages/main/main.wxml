<!--pages/main/main.wxml-->
<view class="linear-box">
  <top-bg></top-bg>
  <view class="card-show-box" style="padding-top:{{navBarHeight+20}}rpx;">
    <navigation-area current-place="{{currentPlace}}"></navigation-area>
    <card-show bindgetCarNum="getCarNum"></card-show>
  </view>
  <!-- 导航 -->
  <busniss-gird class="mg-30"></busniss-gird>
  <!-- 电子车牌 车辆信息 扫一扫按钮 -->
  <group-btn carTotal="{{carTotal}}" isAuth="{{isAuth}}"></group-btn>
  <!-- 活动轮播 -->
  <banner bannerId="1001" class="mb-30"></banner>
</view>

