/* pages/subPages/maint/components/tipDialog.wxss */
.box {
  position: relative;
  width: 630rpx;
  height: 472rpx;
}
.btns {
  margin-top: 53rpx;
  display: flex;
  justify-content: space-between;
  line-height: 80rpx;
  font-size: 32rpx;
}
.common {
  border-radius: 8rpx;
  height: 80rpx;
  width: 48%;
}
.cancel {
  margin-right: 10px;
  background-color: #f5f5f5;
  color: #666;
  text-align: center;
}
.confirm {
  margin-left: 10px;
  background-color: #3E77FF;
  color: #fff;
  text-align: center;
}
.top {
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -315rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  width: 630rpx;
  height: 124rpx;
  background-size: 100%;
  z-index: 1;
}
.content {
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -300rpx;
  color: #4D4D4D;
  font-size: 28rpx;
  box-sizing: border-box;
  width: 600rpx;
  height: 100%;
  padding: 168rpx 34rpx 0 34rpx;
  border-radius: 12rpx;
  background-color: #fff;
}
.top-text {
  margin: 0 auto;
  display: inline-block;
  margin-top: 44rpx;
}
.pl-36 {
  padding-left: 36rpx;
}