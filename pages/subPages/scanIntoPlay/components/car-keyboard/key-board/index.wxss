/* pages/car-keyboard/key-board/index.wxss */
.key-board-wrapper {
  font-size: 32rpx;
  z-index: 999;
  position:fixed;
  bottom:0;
  left:0;
  right:0;
  box-sizing: border-box;
  /* 兼容ios底部安全区域<11 >11 */
  background-color: #F5F9FC;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.key-board-wrapper .key-board-title {
  border-top: 1px solid #ccc;background:#f6f6f6;
  height: 90rpx;
  line-height: 90rpx;
  color:#666;
  text-align:right;
  padding-right: 30rpx
}
.key-board-wrapper .key-board {
  display: grid;
  grid-template-columns: repeat(auto-fill, 64rpx);
  justify-content: center;
  grid-gap: 18rpx 8rpx;
  background:rgba(210,213,219,.9)
}
.last-key-board {
  display: flex;
}
.key-board-wrapper .key {
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing:border-box;
  height: 84rpx;
  background-color:#fff;
  color:#000;
  border-radius: 10rpx;
  box-shadow:0 1px 0 0 rgba(0,0,0,.35)
}
.key-board-wrapper.province-keyboard .key-board {
  padding: 20rpx 16rpx
}
.key-board-wrapper.province-keyboard .key:nth-child(10n) {
  margin-right: 0
}
.key-board-wrapper.province-keyboard .key:nth-child(31) {
  grid-column: span 2;
  background: #dddfe4;
}
.key-board-wrapper.province-keyboard .key:last-child {
  grid-column: span 2;
}
.key-board-wrapper.letter-keyboard .key-board {
  padding: 20rpx 10rpx
}
.key-board-wrapper.letter-keyboard .key:nth-child(31) {
  background:#dddfe4;
}
.key-board-wrapper.letter-keyboard .key.disabled-number:nth-child(-n+10) {
  color:#ccc
}
.key-board-wrapper.letter-keyboard .key.disabled-number:nth-child(19) {
  color:#ccc
}
.key-board-wrapper.letter-keyboard .key.disabled-number:nth-child(25) {
  color:#ccc
}
.key-board-wrapper .key:last-child {
  background: #aeb3bc;
  grid-column: span 2;
}