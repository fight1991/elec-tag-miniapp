<!-- pages/car-keyboard/index.wxml -->
<view>
  <l-key-input plate="{{plate}}" currentIndex="{{currentIndex}}" bind:indexChange="handleIndexChange"></l-key-input>
  <l-carkeyboard wx:if="{{showKeyboard}}" id="keyboard" bind:choose="handlePlateChoose" bind:close="handleClose" bind:delete="handlePlateDelete" disabledNumber="{{disabledNumber}}"></l-carkeyboard>
</view>